{
    "compilerOptions": {
      "declaration": true,
      "target": "ES2020",
      "module": "commonjs",
      "allowJs": true,
      "outDir": "./dist",
      "noEmitOnError": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "jsx": "react",
      "moduleResolution": "node",
    },
    "exclude": [
      "node_modules",
    ]
  }